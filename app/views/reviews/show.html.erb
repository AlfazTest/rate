<div class="container">

  <div class="text-center text-uppercase">
    <h1 class="page-header">Rating</h1>
  </div>

    <p class="col-md-offset-4 text-default">
      <strong>Rating:</strong>
      <%= @review.rating %>
    </p>
  <hr>
    <p class="col-md-offset-4 text-default">
      <strong>Comment:</strong>
      <%= @review.comment %>
    </p>

  <div style="height: 50px"></div>

  <div class="col-md-offset-5">
    <%= link_to 'Edit', edit_review_path(@review),class: "btn btn-success text" %>
    <%= link_to 'Back', reviews_path,class: "btn btn-danger text" %></div>
</div>